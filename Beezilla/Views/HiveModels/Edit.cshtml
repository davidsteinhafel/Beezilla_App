@model Beezilla.Models.HiveModel

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>HiveModel</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="NumberOfFrames" class="control-label"></label>
                <input asp-for="NumberOfFrames" class="form-control" />
                <span asp-validation-for="NumberOfFrames" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Temperament" class="control-label"></label>
                <select asp-for="Temperament" class="form-control">
                    <option value="">Choose One</option>
                    <option value="Aggressive">Aggressive</option>
                    <option value="Calm">Calm</option>
                    <option value="Indifferent">Indifferent</option>
                </select>
            </div>
            <div class="form-group">
                <label asp-for="Brood" class="control-label"></label>
                <input asp-for="Brood" class="form-control" />
                <span asp-validation-for="Brood" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="BroodPattern" class="control-label"></label>
                <select asp-for="BroodPattern" class="form-control">
                    <option value="">Choose One</option>
                    <option value="Spotty">Spotty</option>
                    <option value="Solid">Solid</option>
                    <option value="ConvexCaps">ConvexCaps</option>
                </select>
            </div>
            <div class="form-group">
                <label asp-for="Size" class="control-label"></label>
                <input asp-for="Size" class="form-control" />
                <span asp-validation-for="Size" class="text-danger"></span>
