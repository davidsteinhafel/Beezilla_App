            <div class="form-group">
                <label asp-for="Species" class="control-label"></label>
                <input asp-for="Species" class="form-control" />
                <span asp-validation-for="Species" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Mites" class="control-label"></label>
                <input asp-for="Mites" class="form-control" />
                <span asp-validation-for="Mites" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HiveType" class="control-label"></label>
                <input asp-for="HiveType" class="form-control" />
                <span asp-validation-for="HiveType" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Propolis" class="control-label"></label>
                <select asp-for="Propolis" class="form-control">
                    <option value="">Choose One</option>
                    <option value="Lots">Lots</option>
                    <option value="Average">Average</option>
                    <option value="Little">Little</option>
                </select>
            </div>
            <div class="form-group">
                <label asp-for="HiveImageUrl" class="control-label"></label>
                <input asp-for="HiveImageUrl" class="form-control" />
                <span asp-validation-for="HiveImageUrl" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="QueenCells" class="control-label"></label>
                <input asp-for="QueenCells" class="form-control" />
                <span asp-validation-for="QueenCells" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HiveStrength" class="control-label"></label>
                <input asp-for="HiveStrength" class="form-control" />
                <span asp-validation-for="HiveStrength" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="SwarmPotential" class="control-label"></label>
                <input asp-for="SwarmPotential" class="form-control" />
                <span asp-validation-for="SwarmPotential" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HiveLat" class="control-label"></label>
                <input asp-for="HiveLat" class="form-control" />
